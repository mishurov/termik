<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:key="engine_preference"
        android:summary="%s"
        android:defaultValue="0"
        android:title="Engine"
        android:entries="@array/Engine"
        android:entryValues="@array/EngineData"/>

    <ListPreference
        android:key="output_preference"
        android:summary="%s"
        android:defaultValue="0"
        android:title="Output"
        android:entries="@array/Output"
        android:entryValues="@array/OutputData"/>

</PreferenceScreen>
